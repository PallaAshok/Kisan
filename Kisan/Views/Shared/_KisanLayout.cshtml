<!DOCTYPE html>
<html ng-cloak ng-app="KisanMaster">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <link href="~/Content/md-data-table.css" rel="stylesheet" />
</head>
<body md-theme-watch layout="column">

    @RenderBody()
    @*<div ng-controller="mainCtrl"></div>*@
    @Scripts.Render("~/bundles/angularjs")
    @Scripts.Render("~/bundles/angularMaterial")
    @Scripts.Render("~/bundles/Customjs")
    <script src="~/Scripts/md-data-table.js"></script>
    <script src="~/Scripts/angular-ui-router.js"></script>
    <script src="~/Scripts/Kisan/KisanApp.js"></script>
    <script src="~/Scripts/Kisan/Farmer.js"></script>
    <script src="~/Scripts/Kisan/Dashboard.js"></script>
    @RenderSection("scripts", required: false)
    <style>
        .md-errors-spacer {
            display: none;
        }
        /*md-toolbar, .md-toolbar-tools {
            height: 40px !important;
            max-height:40px !important;
        }*/
    </style>
</body>
</html>
